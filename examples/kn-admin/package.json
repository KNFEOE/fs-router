{
  "name": "kn-admin",
  "version": "1.0.0",
  "license": "ISC",
  "description": "",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "analyze": "pnpm --filter @app/* run analyze",
    "dev": "pnpm --parallel --filter @app/* run dev",
    "build": "pnpm build:packages && pnpm build:apps",
    "build:apps": "pnpm --filter @app/* build",
    "build:shared": "pnpm --filter @kn-admin/shared build",
    "build:packages": "pnpm build:shared",
    "start": "pnpm --parallel --filter @app/* run start",
    "serve": "pnpm --parallel --filter @app/* run serve",
    "deploy": "pnpm --parallel --filter @app/* run deploy",
    "test": "pnpm --parallel --filter @app/* run test",
    "lint": "pnpm --parallel --filter @app/* run lint",
    "clean": "pnpm --filter @app/* run reset",
    "reset": "pnpm --filter @app/* run reset",
    "upgrade": "pnpm --filter @app/* run upgrade"
  },
  "pnpm": {
    "overrides": {
      "react": "19.0.0",
      "react-dom": "19.0.0",
      "react-router": "7.2.0",
      "@loadable/component": "5.16.4",
      "ahook": "3.8.4",
      "react-apexcharts": "1.7.0",
      "@mui/material": "6.4.6",
      "@mui/system": "6.4.6",
      "@emotion/cache": "11.13.1",
      "@emotion/react": "11.14.0",
      "@emotion/styles": "11.13.1",
      "@fontsource-variable/dm-sans": "5.0.7",
      "@fontsource/barlow": "5.0.14",
      "@iconify/react": "5.0.2",
      "@mui/lab": "5.0.0-alpha.173",
      "apexcharts": "3.52.0",
      "dayjs": "1.11.13",
      "history": "5.3.0",
      "simplebar-react": "3.2.6"
    },
    "ignoredBuiltDependencies": [
      "@biomejs/biome",
      "core-js",
      "esbuild"
    ]
  },
  "keywords": []
}