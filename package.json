{
	"name": "@feoe/fs-router",
	"version": "0.0.11",
	"author": "https://github.com/KNFEOE",
	"homepage": "https://knfeoe.github.io/fs-router",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/KNFEOE/fs-router.git"
	},
	"bugs": {
		"url": "https://github.com/KNFEOE/fs-router/issues"
	},
	"description": "file system based routing",
	"keywords": [
		"file-routing",
		"file-routes",
		"file-system-router",
		"fs-router"
	],
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./rspack": {
			"types": "./dist/plugin/rspack.d.ts",
			"import": "./dist/rspack.js",
			"require": "./dist/rspack.cjs"
		},
		"./webpack": {
			"types": "./dist/plugin/webpack.d.ts",
			"import": "./dist/webpack.js",
			"require": "./dist/webpack.cjs"
		},
		"./vite": {
			"types": "./dist/plugin/vite.d.ts",
			"import": "./dist/vite.js",
			"require": "./dist/vite.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": ["dist"],
	"scripts": {
		"build": "rslib build",
		"dev": "rslib build --watch",
		"test": "vitest run",
		"prepare": "simple-git-hooks",
		"prepublishOnly": "pnpm test && pnpm build",
		"docs:dev": "rspress dev docs",
		"docs:build": "rspress build docs --config docs/.rspress/config.ts",
		"docs:preview": "rspress preview docs"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@rslib/core": "^0.4.1",
		"@rspress/theme-default": "^1.45.1",
		"@types/node": "^22.8.1",
		"@types/react": "^18.2.48",
		"react": "^18.2.0",
		"rspress": "^1.45.1",
		"simple-git-hooks": "2.13.0",
		"typescript": "^5.7.3",
		"vite": "^6.2.0",
		"vitest": "^3.0.5",
		"webpack": "^5.98.0"
	},
	"simple-git-hooks": {
		"pre-push": "pnpm test"
	},
	"dependencies": {
		"@types/glob": "^8.1.0",
		"chokidar": "^4.0.3",
		"glob": "^11.0.1",
		"react-router-dom": "^6.30.1",
		"typescript": "^5.0.0",
		"unplugin": "^2.1.2"
	},
	"peerDependencies": {
		"@types/react": "^18.2.48",
		"react": "^18.2.0"
	},
	"license": "MIT",
	"packageManager": "pnpm@10.15.0"
}
